{
	"name": "SAP Network Monitor",
	"version": "2.6",
	"description": "SAP Network Monitor",
	"devtools_page": "networkrequests.html",
	"manifest_version": 2,
	"background": {
        "page":"background_page.html"
    },
	"web_accessible_resources": ["*"],
	"permissions": [
		"tabs",
		"clipboardWrite",
		"http://*/*",
		"https://*/*",
		"https://twitter.com/search?q=test" ,
		"https://google.com/search?search=test",
		"https://developers.google.com/custom-search/json-api/v1/overview",
		"http://services.odata.org/V2/(S(lts2i32bwwy01zoq4cxyscwt))/OData/OData.svc/Products"
	],
	
	
	
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'" ,
	
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	}
}